export const FETCH_POSTS='FETCH_POSTS';
export const NEW_POSTS='NEW_POSTS';

export const fetchPosts =()=>dispatch =>{
    fetch('https://jsonplaceholder.typicode.com/posts')
    .then (res=>res.json())
    .then(posts=>
        dispatch({
            type:FETCH_POSTS,
            payload:posts
        }));
};










}
